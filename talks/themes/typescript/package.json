{
  "name": "slidev-theme-typescript",
  "version": "0.0.0",
  "type": "module",
  "keywords": [
    "slidev-theme",
    "slidev"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "slidev build example.md",
    "dev": "slidev example.md --open",
    "export": "slidev export example.md",
    "screenshot": "slidev export example.md --format png"
  },
  "dependencies": {
    "@slidev/types": "^52.11.3"
  },
  "devDependencies": {
    "@slidev/cli": "^52.11.3"
  },
  "slidev": {
    "colorSchema": "light",
    "defaults": {
      "fonts": {
        "sans": "Noto Sans JP",
        "mono": "Fira Code"
      },
      "themeConfig": {
        "primary": "#0062a3"
      }
    }
  }
}
