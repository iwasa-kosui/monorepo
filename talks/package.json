{
  "name": "talks",
  "type": "module",
  "private": true,
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "wrangler pages dev dist --port 8788",
    "dev:astro": "astro dev",
    "build": "node scripts/build.mjs",
    "build:astro": "astro build",
    "deploy:prod": "wrangler pages deploy dist",
    "dev:2023-document": "slidev 2023/document/slides.md --open",
    "dev:2023-techplay": "slidev 2023/techplay/slides.md --open",
    "dev:2024-techplay-typescript-lt": "slidev 2024/techplay-typescript-lt/slides.md --open",
    "dev:2024-tskaigi": "slidev 2024/tskaigi/slides.md --open",
    "dev:2025-fp-matsuri": "slidev 2025/fp-matsuri/slides.md --open",
    "dev:2026-sre-kaigi-2026": "slidev 2026/sre-kaigi-2026/slides.md --open",
    "build:2023-document": "slidev build 2023/document/slides.md --base /2023/document/ --out 2023/document/dist",
    "build:2023-techplay": "slidev build 2023/techplay/slides.md --base /2023/techplay/ --out 2023/techplay/dist",
    "build:2024-techplay-typescript-lt": "slidev build 2024/techplay-typescript-lt/slides.md --base /2024/techplay-typescript-lt/ --out 2024/techplay-typescript-lt/dist",
    "build:2024-tskaigi": "slidev build 2024/tskaigi/slides.md --base /2024/tskaigi/ --out 2024/tskaigi/dist",
    "build:2025-fp-matsuri": "slidev build 2025/fp-matsuri/slides.md --base /2025/fp-matsuri/ --out 2025/fp-matsuri/dist",
    "build:2026-sre-kaigi-2026": "slidev build 2026/sre-kaigi-2026/slides.md --base /2026/sre-kaigi-2026/ --out 2026/sre-kaigi-2026/dist",
    "export:2023-document": "slidev export 2023/document/slides.md",
    "export:2023-techplay": "slidev export 2023/techplay/slides.md",
    "export:2024-techplay-typescript-lt": "slidev export 2024/techplay-typescript-lt/slides.md",
    "export:2024-tskaigi": "slidev export 2024/tskaigi/slides.md",
    "export:2025-fp-matsuri": "slidev export 2025/fp-matsuri/slides.md",
    "export:2026-sre-kaigi-2026": "slidev export 2026/sre-kaigi-2026/slides.md",
    "dev:theme-kosui": "slidev themes/kosui/example.md --open",
    "dev:theme-kkhs-marp": "slidev themes/kkhs-marp/example.md --open",
    "dev:theme-typescript": "slidev themes/typescript/example.md --open",
    "dev:theme-tskaigi": "slidev themes/tskaigi/example.md --open",
    "dev:theme-fp-matsuri": "slidev themes/fp-matsuri/example.md --open",
    "dev:theme-kkhs": "slidev themes/kkhs/example.md --open"
  },
  "dependencies": {
    "@astrojs/tailwind": "^6.0.2",
    "@iconify-json/bi": "^1.1.23",
    "@iconify-json/mdi": "^1.2.3",
    "@iconify-json/tabler": "^1.1.110",
    "@iconify-json/vscode-icons": "^1.1.33",
    "@slidev/cli": "^52.11.3",
    "@slidev/theme-default": "latest",
    "@slidev/theme-seriph": "latest",
    "@slidev/types": "^52.11.3",
    "astro": "^5.5.3",
    "fp-ts": "2.16.5",
    "markdown-it-github-alerts": "1.0.1",
    "newtype-ts": "0.3.5",
    "open-graph-scraper": "6.11.0",
    "tailwindcss": "^3.4.17",
    "vue": "^3.5.26"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.15.3",
    "js-yaml": "^4.1.0",
    "playwright-chromium": "^1.57.0",
    "wrangler": "^4.20.0"
  }
